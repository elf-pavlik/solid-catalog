{
  "name": "Solid Resources Catalog",
  "version": "2.0.0",
  "type": "module",
  "description": "-- a listing of things useful to the Solid Community",
  "main": "catalog.html",
  "scripts": {
    "test": "rdf-dereference ./catalog-shacl.ttl && rdf-dereference ./catalog-skos.ttl && rdf-dereference ./catalog-data.ttl",
    "build": "rdf-transform-cli ./catalog-shacl.shce ./catalog-shacl.ttl -p"
  },
  "keywords": [],
  "author": "Jeff Zucker",
  "license": "MIT",
  "dependencies": {
    "@solid-rest/file": "^2.1.20",
    "rdflib": "^2.2.37"
  },
  "devDependencies": {
    "rdf-dereference": "^4.0.0",
    "rdf-transform-cli": "^1.1.0",
    "semantic-release": "^24.2.5"
  },
  "release": {
    "branches": [
      "main",
      "+([0-9])?(.{+([0-9]),x}).x",
      "next",
      {
        "name": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      }
    ],
    "repositoryUrl": "https://github.com/solid/catalo"
  }
}
